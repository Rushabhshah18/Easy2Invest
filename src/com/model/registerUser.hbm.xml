<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.model">
	<class name="registerUser" table="main_user">
		<id column="id" name="id">
			<generator class="increment"></generator>
		</id>
		<property column="fullname" name="fullname"></property>
		<property column="city" name="city"></property>
		<property column="email" name="email"></property>
		<property column="gender" name="gender"></property>
		<property column="status" name="status"></property>
		<property column="password" name="password"></property>
		<property column="active_flag" name="active_flag"></property>
		<property column="birthdate" name="birthdate"></property>
		<property column="userType" name="userType"></property>
		<property column="otp" name="otp"></property>	
	</class>
</hibernate-mapping>
